<?xml version="1.0" encoding="UTF-8"?>
<xs:schema xmlns:xs="http://www.w3.org/2001/XMLSchema" targetNamespace="http://xml.amadeus.com/PALXRQ_13_1_1A" xmlns="http://xml.amadeus.com/PALXRQ_13_1_1A" elementFormDefault="qualified">
  <xs:element name="PNR_ListCancelledPassengersByFlight">
    <xs:annotation>
      <xs:documentation xml:lang="en">Pnr Active List Cancelled Passengers Retrieve reQuest
Search a flight/date for a list of PNRs matching a set of criteria
</xs:documentation>
    </xs:annotation>
    <xs:complexType>
      <xs:sequence>
        <xs:element name="flightDateQuery">
          <xs:annotation>
            <xs:documentation xml:lang="en">This group contains all input parameters.</xs:documentation>
          </xs:annotation>
          <xs:complexType>
            <xs:sequence>
              <xs:element name="flightIdentification" type="FlightDetailsQueryType">
                <xs:annotation>
                  <xs:documentation xml:lang="en">Main search criterion: flight number and Airline Code</xs:documentation>
                </xs:annotation>
              </xs:element>
              <xs:element name="dateIdentification" type="StructuredDateTimeInformationType">
                <xs:annotation>
                  <xs:documentation xml:lang="en">Main search criterion: flight date</xs:documentation>
                </xs:annotation>
              </xs:element>
              <xs:element name="outputSelectionOption">
                <xs:annotation>
                  <xs:documentation xml:lang="en">Drives the content of the response.</xs:documentation>
                </xs:annotation>
                <xs:complexType>
                  <xs:sequence>
                    <xs:element name="outputType" type="SelectionDetailsTypeI">
                      <xs:annotation>
                        <xs:documentation xml:lang="en">To select Output: PNRs Counters only PNRs Counters and PNRs images. Queue-Placed PNRs</xs:documentation>
                      </xs:annotation>
                    </xs:element>
                    <xs:element name="elementType" minOccurs="0" maxOccurs="20" type="ElementManagementSegmentType">
                      <xs:annotation>
                        <xs:documentation xml:lang="en">Used for specifying the PNR data elements to be included in the response</xs:documentation>
                      </xs:annotation>
                    </xs:element>
                  </xs:sequence>
                </xs:complexType>
              </xs:element>
              <xs:element name="searchCriteria" minOccurs="0" maxOccurs="10">
                <xs:annotation>
                  <xs:documentation xml:lang="en">Primary and secondary search criteria. Logical AND between primary search criteria is performed implicitly</xs:documentation>
                </xs:annotation>
                <xs:complexType>
                  <xs:sequence>
                    <xs:element name="primarySearchCriterion" type="SelectionDetailsTypeI">
                      <xs:annotation>
                        <xs:documentation xml:lang="en">A primary search criterion can be expressed as:   - a functional rule based on different PNR elements (e.g. un-ticketed passengers, based on the absence of TKOK or SSR TKNE, SSR TKNA, SSR TKNM).   - the presence of a given PNR element (e.g. passengers having a SSR)   - a particular PNR characteristic (e.g. group PNR).</xs:documentation>
                      </xs:annotation>
                    </xs:element>
                    <xs:element name="negativeMode" type="BooleanExpressionRuleType">
                      <xs:annotation>
                        <xs:documentation xml:lang="en">Indicates how the primary search criterion has to be interpreted (perform a positive or a negative search):  - NOT: negate the primary search criterion  - NOP: don't negate the primary search criterion</xs:documentation>
                      </xs:annotation>
                    </xs:element>
                    <xs:element name="associationMode" minOccurs="0" type="BooleanExpressionRuleType">
                      <xs:annotation>
                        <xs:documentation xml:lang="en">Indicates how the secondary search criteria must be combined (logical AND or logical OR between them)</xs:documentation>
                      </xs:annotation>
                    </xs:element>
                    <xs:element name="secondarySearchCriteria" minOccurs="0" maxOccurs="10">
                      <xs:annotation>
                        <xs:documentation xml:lang="en">This group is able to convey any secondary search criteria (i.e. search values). A secondary search criterion can consist of:  - a PNR element type (e.g. FB element). In this case, EMS segment is used  - the actual value of a business object (e.g. class of service).  Segments in this group are mutually exclusive.</xs:documentation>
                      </xs:annotation>
                      <xs:complexType>
                        <xs:sequence>
                          <xs:element name="triggerMarker" type="DummySegmentTypeI">
                            <xs:annotation>
                              <xs:documentation xml:lang="en">This segment is here as trigger because all other segments that follow are mutually exclusive.</xs:documentation>
                            </xs:annotation>
                          </xs:element>
                          <xs:element name="classOfServiceValue" minOccurs="0" type="ProductInformationTypeI">
                            <xs:annotation>
                              <xs:documentation xml:lang="en">Used for a search by class of Service.</xs:documentation>
                            </xs:annotation>
                          </xs:element>
                          <xs:element name="carrierOrFlightValue" minOccurs="0" type="FlightDetailsQueryType_133632S">
                            <xs:annotation>
                              <xs:documentation xml:lang="en">Used for a search by:   - inbound/outbound connection   - marketing carrier/flight In both cases a carrier code or a full flight number can be specified.  Composites are conditional because they are used in mutually exclusive situations</xs:documentation>
                            </xs:annotation>
                          </xs:element>
                          <xs:element name="ssrOsiSkValue" minOccurs="0" type="SpecialRequirementsDetailsType">
                            <xs:annotation>
                              <xs:documentation xml:lang="en">Used for a search for:   - specific SSR/OSI/SK type   - seat number  Composites are conditional because they are used in mutually exclusive situations</xs:documentation>
                            </xs:annotation>
                          </xs:element>
                          <xs:element name="cabinValue" minOccurs="0" type="SegmentCabinIdentificationType">
                            <xs:annotation>
                              <xs:documentation xml:lang="en">Used for a search by cabin</xs:documentation>
                            </xs:annotation>
                          </xs:element>
                          <xs:element name="statusCodeValue" minOccurs="0" type="RelatedProductInformationTypeI">
                            <xs:annotation>
                              <xs:documentation xml:lang="en">Used for a search by status code.</xs:documentation>
                            </xs:annotation>
                          </xs:element>
                          <xs:element name="passengerNameOrTypeValue" minOccurs="0" type="TravellerInformationType">
                            <xs:annotation>
                              <xs:documentation xml:lang="en">Used for a search by:   - passenger last name   - passenger type   - name type in a group PNR (corporate or individual name)  Composites are conditional because they are used in mutually exclusive situations</xs:documentation>
                            </xs:annotation>
                          </xs:element>
                          <xs:element name="originValue" minOccurs="0" type="AdditionalBusinessSourceInformationType">
                            <xs:annotation>
                              <xs:documentation xml:lang="en">Used for a serach by:   - PNR Owner   - POS (system or country)   - AP element (City code)  Composites are conditional because they are used in mutually exclusive situations</xs:documentation>
                            </xs:annotation>
                          </xs:element>
                          <xs:element name="ticketArrangementValue" minOccurs="0" type="StatusTypeI">
                            <xs:annotation>
                              <xs:documentation xml:lang="en">Used when searching for:   - ticketed passengers   - unticketed passengers</xs:documentation>
                            </xs:annotation>
                          </xs:element>
                          <xs:element name="numericRangeValue" minOccurs="0" type="RangeDetailsTypeI">
                            <xs:annotation>
                              <xs:documentation xml:lang="en">Used for a search by:   - connection time (e.g. less than 4 hours)   - PNR number in party (e.g. NIP between 2 and 8)   - number of passengers (e.g. 10 last booked passengers)</xs:documentation>
                            </xs:annotation>
                          </xs:element>
                          <xs:element name="elementTypeValue" minOccurs="0" type="ElementManagementSegmentType_133630S">
                            <xs:annotation>
                              <xs:documentation xml:lang="en">Used when searching for a specific PNR element type, without being interested in its content (e.g. Primary search criterion is "ticketed passengers" and secondary search criterion is "passenger has a FA element")</xs:documentation>
                            </xs:annotation>
                          </xs:element>
                          <xs:element name="frequentFlyerValue" minOccurs="0" type="FrequentTravellerIdentificationCodeType">
                            <xs:annotation>
                              <xs:documentation xml:lang="en">Used for a search by:  - airline priority code  - airline tier level  - alliance tier description  Data elements are conditional because they are used in mutually exclusive situations.</xs:documentation>
                            </xs:annotation>
                          </xs:element>
                          <xs:element name="negoRecordLocator" minOccurs="0" type="NegotiatedspaceKeyInformationType">
                            <xs:annotation>
                              <xs:documentation xml:lang="en">Unique ID for negociated space</xs:documentation>
                            </xs:annotation>
                          </xs:element>
                          <xs:element name="extendedPassengerName" minOccurs="0" type="EnhancedTravellerInformationType">
                            <xs:annotation>
                              <xs:documentation xml:lang="en">To Filter Search using Passenger Name - Supporting Non-Roman Scripts (UTF-8)</xs:documentation>
                            </xs:annotation>
                          </xs:element>
                        </xs:sequence>
                      </xs:complexType>
                    </xs:element>
                  </xs:sequence>
                </xs:complexType>
              </xs:element>
              <xs:element name="outputAggregationOption" minOccurs="0" maxOccurs="5">
                <xs:annotation>
                  <xs:documentation xml:lang="en">To select the way output data have to be aggregated (e.g. first by flight segment, then by class). When this group is not present in the request, the output is "flat" (no specific aggregation strategy is applied).</xs:documentation>
                </xs:annotation>
                <xs:complexType>
                  <xs:sequence>
                    <xs:element name="aggregationLevel" type="LevelIndicationTypeU">
                      <xs:annotation>
                        <xs:documentation xml:lang="en">Position of the aggregation key in the aggregation hierarchy.</xs:documentation>
                      </xs:annotation>
                    </xs:element>
                    <xs:element name="aggregationKey" type="SelectionDetailsTypeI">
                      <xs:annotation>
                        <xs:documentation xml:lang="en">Aggregation key (e.g. aggregate by class)</xs:documentation>
                      </xs:annotation>
                    </xs:element>
                  </xs:sequence>
                </xs:complexType>
              </xs:element>
              <xs:element name="queueDetails" minOccurs="0">
                <xs:annotation>
                  <xs:documentation xml:lang="en">To convey Queue Details when Queue is selected as Output mode.</xs:documentation>
                </xs:annotation>
                <xs:complexType>
                  <xs:sequence>
                    <xs:element name="queue" type="QueueInformationTypeI">
                      <xs:annotation>
                        <xs:documentation xml:lang="en">Queue Number</xs:documentation>
                      </xs:annotation>
                    </xs:element>
                    <xs:element name="category" type="SubQueueInformationTypeI">
                      <xs:annotation>
                        <xs:documentation xml:lang="en">Queue Category</xs:documentation>
                      </xs:annotation>
                    </xs:element>
                  </xs:sequence>
                </xs:complexType>
              </xs:element>
            </xs:sequence>
          </xs:complexType>
        </xs:element>
      </xs:sequence>
    </xs:complexType>
  </xs:element>
  <xs:complexType name="AdditionalBusinessSourceInformationType">
    <xs:annotation>
      <xs:documentation xml:lang="en">To specify ownership and creator of a PNR</xs:documentation>
