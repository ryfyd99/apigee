<?xml version="1.0" encoding="UTF-8"?>
<xs:schema xmlns:xs="http://www.w3.org/2001/XMLSchema" targetNamespace="http://xml.amadeus.com/ACSFRR_11_6_1A" xmlns="http://xml.amadeus.com/ACSFRR_11_6_1A" elementFormDefault="qualified">
  <xs:element name="DCSLST_GetCheckInSummaryFiguresReply">
    <xs:annotation>
      <xs:documentation xml:lang="en">To return passenger figures related to check-in according to criteria given in the query message.</xs:documentation>
    </xs:annotation>
    <xs:complexType>
      <xs:sequence>
        <xs:element name="errors" minOccurs="0" maxOccurs="5" type="ErrorGroupType">
          <xs:annotation>
            <xs:documentation xml:lang="en">Errors and warnings</xs:documentation>
          </xs:annotation>
        </xs:element>
        <xs:element name="flightDetailsGroup" minOccurs="0">
          <xs:annotation>
            <xs:documentation xml:lang="en">Used to convey the information of the flight and the figures</xs:documentation>
          </xs:annotation>
          <xs:complexType>
            <xs:sequence>
              <xs:element name="flightLegInformation" type="FlightDetailsResponseType">
                <xs:annotation>
                  <xs:documentation xml:lang="en">Used to convey the operational flight details for the leg: - Operating Carrier - Operating Flight Suffix - Operating Flight Number - Standard Departure Date - Standard Departure Time - Standard Arrival Date - Standard Arrival Time - Boardpoint - Offpoint</xs:documentation>
                </xs:annotation>
              </xs:element>
              <xs:element name="versions" minOccurs="0" maxOccurs="4" type="ItemReferencesAndVersionsType">
                <xs:annotation>
                  <xs:documentation xml:lang="en">Versions sent to CM stating in which version the system is:  - SV: Status Version - TV: Timing Version - DV: Leg Data Version</xs:documentation>
                </xs:annotation>
              </xs:element>
              <xs:element name="flightStatus" maxOccurs="18" type="StatusType">
                <xs:annotation>
                  <xs:documentation xml:lang="en">To specify: - Acceptance flight status  (1245)AC - General flight status  (1245) GN - Load control status  (1245) LC - Boarding flight status  (1245) BO - Baggage Status  (1245) BG - Security Discrepancies (ATL/ATT) exist  (1245) SDE - Baggage Discrepancies (BNS/Offload) exist (1245) BDE - Flight Baggage Status has been Reopened (1245) BSR - Transfer Through Checkin Indicator (1245) TCI - BTC Bypass Customer Type Check - BSC Bypass Sequence Check - BGC Bypass Goshow check - MOV Moveable curtain flight indicator - VDB VBD feature available ( itemisation ) - VDG VDG feature available ( itemisation ) - DIS (disruption status) - OS (OnLoadFlightStatus) - POI (Pris d'Office Indicator)</xs:documentation>
                </xs:annotation>
              </xs:element>
              <xs:element name="flightDetails" minOccurs="0" maxOccurs="99">
                <xs:annotation>
                  <xs:documentation xml:lang="en">To hold the marketing and seating cutoff indicator per codeshare partner.</xs:documentation>
                </xs:annotation>
                <xs:complexType>
                  <xs:sequence>
                    <xs:element name="marketingFlightDetails" type="CommercialAgreementsTypeI">
                      <xs:annotation>
                        <xs:documentation xml:lang="en">To specify: - Marketing Carrier - Marketing Flight Suffix - Marketing Flight Number Is used when the figures are not split by codeshare partners.</xs:documentation>
                      </xs:annotation>
                    </xs:element>
                    <xs:element name="seatingCutOffIndicator" minOccurs="0" type="StatusType">
                      <xs:annotation>
                        <xs:documentation xml:lang="en">true if the codeshare blockspace seating limit has been reached. 1245=SCR</xs:documentation>
                      </xs:annotation>
                    </xs:element>
                  </xs:sequence>
                </xs:complexType>
              </xs:element>
              <xs:element name="preDefinedComment" minOccurs="0">
                <xs:annotation>
                  <xs:documentation xml:lang="en">To convey the pre-defined comment associated to the leg.</xs:documentation>
                </xs:annotation>
                <xs:complexType>
                  <xs:sequence>
                    <xs:element name="preDefinedCommentNumber" type="NumberOfUnitsType">
                      <xs:annotation>
                        <xs:documentation xml:lang="en">The number of the pre-defined comment.</xs:documentation>
                      </xs:annotation>
                    </xs:element>
                    <xs:element name="preDefinedCommentText" type="FreeTextInformationType_388S">
                      <xs:annotation>
                        <xs:documentation xml:lang="en">The text of the pre-defined comment.</xs:documentation>
                      </xs:annotation>
                    </xs:element>
                  </xs:sequence>
                </xs:complexType>
              </xs:element>
              <xs:element name="flightRelatedComments" minOccurs="0" maxOccurs="999">
                <xs:annotation>
                  <xs:documentation xml:lang="en">To convey the standard comments associated to the leg.</xs:documentation>
                </xs:annotation>
                <xs:complexType>
                  <xs:sequence>
                    <xs:element name="commentId" type="ItemReferencesAndVersionsType_123290S">
                      <xs:annotation>
                        <xs:documentation xml:lang="en">The unique comment identifier (URI).</xs:documentation>
                      </xs:annotation>
                    </xs:element>
                    <xs:element name="commentText" type="FreeTextInformationType_388S">
                      <xs:annotation>
                        <xs:documentation xml:lang="en">The text of the standard comment.</xs:documentation>
                      </xs:annotation>
                    </xs:element>
                    <xs:element name="commentCategory" minOccurs="0" maxOccurs="10" type="StatusType">
                      <xs:annotation>
                        <xs:documentation xml:lang="en">To convey the category(ies) of the comment.</xs:documentation>
                      </xs:annotation>
                    </xs:element>
                  </xs:sequence>
                </xs:complexType>
              </xs:element>
              <xs:element name="flightTimings" minOccurs="0" maxOccurs="2" type="StructuredDateTimeInformationType">
                <xs:annotation>
                  <xs:documentation xml:lang="en">To specify: - Boarding Time (2005=BOT)</xs:documentation>
                </xs:annotation>
              </xs:element>
              <xs:element name="legInformation" maxOccurs="99">
                <xs:annotation>
                  <xs:documentation xml:lang="en">To specify leg and segment information for the check-in summary list.</xs:documentation>
                </xs:annotation>
                <xs:complexType>
                  <xs:sequence>
                    <xs:element name="legRestriction" type="OriginAndDestinationDetailsTypeI">
                      <xs:annotation>
                        <xs:documentation xml:lang="en">To convey the Leg and segment on which the figures apply.</xs:documentation>
                      </xs:annotation>
                    </xs:element>
                    <xs:element name="inhibitIndicators" maxOccurs="4" type="StatusType">
                      <xs:annotation>
                        <xs:documentation xml:lang="en">To specify the Inhibit Indicators: - APP (1245=APP) - AQQ (1245=AQQ) - 1229=1 if the indicator is ON, 0 otherwise</xs:documentation>
                      </xs:annotation>
                    </xs:element>
                    <xs:element name="legFigures" maxOccurs="3" type="NumberOfUnitsType_122540S">
                      <xs:annotation>
                        <xs:documentation xml:lang="en">To specify: - Number of Technical Crew (6353=TCR) - Number of Cabin Crew (6353=CCR) - Infant Quota (6353=INQ)</xs:documentation>
                      </xs:annotation>
                    </xs:element>
                    <xs:element name="weightAllowance" minOccurs="0" type="QuantityType">
                      <xs:annotation>
                        <xs:documentation xml:lang="en">To specify weight allowance (Passengers + Baggages)</xs:documentation>
                      </xs:annotation>
                    </xs:element>
                    <xs:element name="marketingCarrierInfo" maxOccurs="99">
                      <xs:annotation>
                        <xs:documentation xml:lang="en">To specify the flight and check-in data details either once or for each marketing airline.</xs:documentation>
                      </xs:annotation>
                      <xs:complexType>
                        <xs:sequence>
                          <xs:element name="codeshareCarrier" type="TransportIdentifierType">
                            <xs:annotation>
                              <xs:documentation xml:lang="en">To specify Marketing Carrier</xs:documentation>
                            </xs:annotation>
                          </xs:element>
                          <xs:element name="seatNumCounts" maxOccurs="2" type="NumberOfUnitsType_122540S">
                            <xs:annotation>
                              <xs:documentation xml:lang="en">To specify: - Number of Joining Jump Seat Customers Accepted (JJA) - Number of Transit Jump Seat Customers Accepted (JTA)</xs:documentation>
                            </xs:annotation>
                          </xs:element>
                          <xs:element name="legCabinInfo" maxOccurs="9">
                            <xs:annotation>
                              <xs:documentation xml:lang="en">To specify the figures specific to each cabin.</xs:documentation>
                            </xs:annotation>
                            <xs:complexType>
                              <xs:sequence>
                                <xs:element name="cabinAndServiceClass" type="CabinDetailsType_78486S">
                                  <xs:annotation>
                                    <xs:documentation xml:lang="en">To specify the cabin code</xs:documentation>
                                  </xs:annotation>
                                </xs:element>
                                <xs:element name="cabinFigures" maxOccurs="84" type="NumberOfUnitsType_122540S">
                                  <xs:annotation>
                                    <xs:documentation xml:lang="en">Provides the customer figures for each customer counter type. There are counters for customers, bookable staff and rebate staff.</xs:documentation>
                                  </xs:annotation>
                                </xs:element>
                              </xs:sequence>
                            </xs:complexType>
                          </xs:element>
                        </xs:sequence>
                      </xs:complexType>
                    </xs:element>
                  </xs:sequence>
                </xs:complexType>
              </xs:element>
              <xs:element name="bagIndicators" maxOccurs="4" type="StatusType">
                <xs:annotation>
                  <xs:documentation xml:lang="en">To specify: - Hold Baggage Weight Indicator (1245=HOW) - Hand Baggage Weight Indicator (1245=HBW) - Hand Baggage Pieces Indicator (1245=HBP) - Hold Baggage Pieces Indicator (1245=HDP)</xs:documentation>
                </xs:annotation>
              </xs:element>
              <xs:element name="cabinTotalInfo" maxOccurs="9">
                <xs:annotation>
                  <xs:documentation xml:lang="en">Conveys the total figures by cabin.</xs:documentation>
                </xs:annotation>
                <xs:complexType>
                  <xs:sequence>
                    <xs:element name="cabin" type="CabinDetailsType">
                      <xs:annotation>
                        <xs:documentation xml:lang="en">Conveys the cabin code</xs:documentation>
                      </xs:annotation>
                    </xs:element>
                    <xs:element name="cabinFigures" maxOccurs="7" type="NumberOfUnitsType_122540S">
                      <xs:annotation>
                        <xs:documentation xml:lang="en">To specify: - Number of Bookings (6353=BOO) - Number of Customers Accepted (6353=CA) - Number of Customers on Standby (6353=CS) - Number of Transit Customers Booked (6353=TCB) - Number of Transit Customers Accepted (6353=TCA) - Number of Transit Customers on Standby (6353=TCS) - Number of Infant customers Accepted (6353=ITA)</xs:documentation>
                      </xs:annotation>
                    </xs:element>
                    <xs:element name="voluntaryStatus" maxOccurs="2" type="StatusType">
                      <xs:annotation>
                        <xs:documentation xml:lang="en">Conveys VDB and VDG indicators</xs:documentation>
                      </xs:annotation>
                    </xs:element>
                  </xs:sequence>
                </xs:complexType>
              </xs:element>
              <xs:element name="dummySTX" type="DummySegmentTypeI">
                <xs:annotation>
                  <xs:documentation xml:lang="en">to seperate the STX from Group 9.</xs:documentation>
                </xs:annotation>
              </xs:element>
              <xs:element name="contactDetailsIndicator" type="StatusType">
                <xs:annotation>
                  <xs:documentation xml:lang="en">Conveys whether the contact details are available.</xs:documentation>
                </xs:annotation>
              </xs:element>
              <xs:element name="legInformationGroup" maxOccurs="20">
                <xs:annotation>
                  <xs:documentation xml:lang="en">To convey routing and aircraft information for each leg of the flight.  Last repetition of the group contains info for the offpoint of the flight.</xs:documentation>
                </xs:annotation>
                <xs:complexType>
                  <xs:sequence>
                    <xs:element name="terminalInfo" type="AirportNodeInformationType">
                      <xs:annotation>
                        <xs:documentation xml:lang="en">Used to specify: - Airport Code - Airport Terminal Number</xs:documentation>
                      </xs:annotation>
                    </xs:element>
                    <xs:element name="gate" minOccurs="0" maxOccurs="5" type="TerminalLocationType_13164S">
                      <xs:annotation>
                        <xs:documentation xml:lang="en">Gate Location (GTE)</xs:documentation>
                      </xs:annotation>
                    </xs:element>
                    <xs:element name="aircraftInformation" minOccurs="0" type="EquipmentInformationType">
                      <xs:annotation>
                        <xs:documentation xml:lang="en">To specify: - Aircraft Configuration Version - Aircraft Type - Aircraft Registration - Aircraft Owner</xs:documentation>
                      </xs:annotation>
                    </xs:element>
                    <xs:element name="spurNumber" minOccurs="0" type="NumberOfUnitsType">
                      <xs:annotation>
                        <xs:documentation xml:lang="en">To specify the spur number.</xs:documentation>
                      </xs:annotation>
                    </xs:element>
                    <xs:element name="dummyEqn" type="DummySegmentTypeI">
                      <xs:annotation>
                        <xs:documentation xml:lang="en">Used to make difference between EQNs</xs:documentation>
                      </xs:annotation>
                    </xs:element>
                    <xs:element name="legTimings" minOccurs="0" maxOccurs="8" type="StructuredDateTimeInformationType">
                      <xs:annotation>
                        <xs:documentation xml:lang="en">To specify: - Advice Time (2005=ADV) - Actual Departure Time (2005=ATD) - Actual Arrival Time (2005=ATA) - Estimated Departure Time (2005=ETD) - Standard Departure Time (2005=STD) - Estimated Arrival Time (2005=ETA) - Standard Arrival Time (2005=STA) - Original Departure Time (2005=OTD, used when flight was disrupted)</xs:documentation>
                      </xs:annotation>
                    </xs:element>
                    <xs:element name="elapsedPeriods" minOccurs="0" maxOccurs="2" type="NumberOfUnitsType_122569S">
                      <xs:annotation>
                        <xs:documentation xml:lang="en">To specify: - Elapsed Ground Time in HHMM (6353 = EGT) - Elapsed Flying Time in MMMM (6353 = EFT)</xs:documentation>
                      </xs:annotation>
                    </xs:element>
                    <xs:element name="cabinInformation" minOccurs="0" maxOccurs="4">
                      <xs:annotation>
                        <xs:documentation xml:lang="en">used to convey the number of seats and number of meals per cabin</xs:documentation>
                      </xs:annotation>
                      <xs:complexType>
                        <xs:sequence>
                          <xs:element name="aircraftConfiguration" type="EquipmentInformationTypeI">
                            <xs:annotation>
                              <xs:documentation xml:lang="en">To convey: - the cabin class  - the number of seats per class.</xs:documentation>
                            </xs:annotation>
                          </xs:element>
                          <xs:element name="cabinFigure" minOccurs="0" maxOccurs="2" type="NumberOfUnitsType_122540S">
                            <xs:annotation>
                              <xs:documentation xml:lang="en">Catering figure in this cabin if available 6353 = CAT  Standby Staff Limit if available 6353 = SSL</xs:documentation>
                            </xs:annotation>
                          </xs:element>
                          <xs:element name="freeSeatingIndicator" minOccurs="0" type="StatusType">
                            <xs:annotation>
                              <xs:documentation xml:lang="en">Conveys the free seating indicator (FSR).</xs:documentation>
                            </xs:annotation>
                          </xs:element>
                        </xs:sequence>
                      </xs:complexType>
                    </xs:element>
                    <xs:element name="acLocationTerminalDetails" minOccurs="0">
                      <xs:annotation>
                        <xs:documentation xml:lang="en">To convey: - the departure Aircraft Parking Location</xs:documentation>
                      </xs:annotation>
                      <xs:complexType>
                        <xs:sequence>
                          <xs:element name="arrivalDepartureOption" type="ActionIdentificationType">
                            <xs:annotation>
                              <xs:documentation xml:lang="en">To convey: - departure Aircraft Parking Location (1229:DAL)</xs:documentation>
                            </xs:annotation>
                          </xs:element>
                          <xs:element name="aircraftLocation" type="TerminalLocationType">
                            <xs:annotation>
                              <xs:documentation xml:lang="en">To convey the location.  eg:</xs:documentation>
                            </xs:annotation>
                          </xs:element>
                        </xs:sequence>
                      </xs:complexType>
                    </xs:element>
                  </xs:sequence>
                </xs:complexType>
              </xs:element>
            </xs:sequence>
          </xs:complexType>
        </xs:element>
      </xs:sequence>
    </xs:complexType>
  </xs:element>
  <xs:complexType name="ActionIdentificationType">
    <xs:annotation>
      <xs:documentation xml:lang="en">To provide details of action required or taken, the flights to which this action refers, and reasons for action taken.</xs:documentation>
    </xs:annotation>
    <xs:sequence>
      <xs:element name="actionRequestCode" type="AlphaNumericString_Length1To3">
        <xs:annotation>
          <xs:documentation xml:lang="en">provides type of required action, coded.</xs:documentation>
        </xs:annotation>
      </xs:element>
    </xs:sequence>
  </xs:complexType>
  <xs:complexType name="AirportNodeInformationType">
    <xs:annotation>
      <xs:documentation xml:lang="en">To convey information linked to a specific Airport and one or more terminals.</xs:documentation>
    </xs:annotation>
    <xs:sequence>
      <xs:element name="airportCode" type="AlphaString_Length3To3">
        <xs:annotation>
          <xs:documentation xml:lang="en">To specify either a City Code or an Airport Code. See IATA Airline Coding directory. IATA 3 letter city/aircode code</xs:documentation>
        </xs:annotation>
      </xs:element>
      <xs:element name="terminalInformation" minOccurs="0" type="TerminalInformationTypeI">
        <xs:annotation>
          <xs:documentation xml:lang="en">To convey the name of the Terminal.</xs:documentation>
        </xs:annotation>
      </xs:element>
    </xs:sequence>
  </xs:complexType>
  <xs:complexType name="ApplicationErrorDetailType">
    <xs:sequence>
      <xs:element name="errorCode" type="AlphaNumericString_Length1To5">
        <xs:annotation>
          <xs:documentation xml:lang="en">Code identifying the data validation error condition.</xs:documentation>
        </xs:annotation>
      </xs:element>
      <xs:element name="errorCategory" minOccurs="0" type="AlphaNumericString_Length1To3">
        <xs:annotation>
          <xs:documentation xml:lang="en">Identification of a code list.</xs:documentation>
        </xs:annotation>
      </xs:element>
      <xs:element name="errorCodeOwner" minOccurs="0" type="AlphaNumericString_Length1To3">
        <xs:annotation>
          <xs:documentation xml:lang="en">Code identifying the agency responsible for a code list.</xs:documentation>
        </xs:annotation>
      </xs:element>
    </xs:sequence>
  </xs:complexType>
  <xs:complexType name="ApplicationErrorInformationType">
    <xs:annotation>
      <xs:documentation xml:lang="en">To identify the type of application error within a message.</xs:documentation>
    </xs:annotation>
    <xs:sequence>
      <xs:element name="errorDetails" type="ApplicationErrorDetailType">
        <xs:annotation>
          <xs:documentation xml:lang="en">Application error details.</xs:documentation>
        </xs:annotation>
      </xs:element>
    </xs:sequence>
  </xs:complexType>
  <xs:complexType name="CabinClassDesignationType">
    <xs:annotation>
      <xs:documentation xml:lang="en">CABIN CLASS DESIGNATION</xs:documentation>
    </xs:annotation>
    <xs:sequence>
      <xs:element name="classDesignator" minOccurs="0" type="AlphaString_Length1To1">
        <xs:annotation>
          <xs:documentation xml:lang="en">Designates the class of service on the means of transport  in which the passenger will travel</xs:documentation>
        </xs:annotation>
      </xs:element>
      <xs:element name="bookingClass" type="AlphaNumericString_Length1To2">
        <xs:annotation>
          <xs:documentation xml:lang="en">Reservation booking designator</xs:documentation>
        </xs:annotation>
      </xs:element>
    </xs:sequence>
  </xs:complexType>
  <xs:complexType name="CabinDetailsType">
    <xs:annotation>
      <xs:documentation xml:lang="en">To specify the physical layout of a cabin class or cabin compartment of an aircraft.</xs:documentation>
    </xs:annotation>
    <xs:sequence>
      <xs:element name="cabinDetails" type="CabinClassDesignationType">
        <xs:annotation>
          <xs:documentation xml:lang="en">Cabin class designation</xs:documentation>
        </xs:annotation>
      </xs:element>
    </xs:sequence>
  </xs:complexType>
  <xs:complexType name="CabinDetailsType_78486S">
    <xs:annotation>
      <xs:documentation xml:lang="en">To specify the physical layout of a cabin class or cabin compartment of an aircraft.</xs:documentation>
    </xs:annotation>
    <xs:sequence>
      <xs:element name="cabinDetails" type="CabinClassDesignationType">
        <xs:annotation>
          <xs:documentation xml:lang="en">Cabin class designation</xs:documentation>
        </xs:annotation>
      </xs:element>
      <xs:element name="serviceClass" minOccurs="0" type="AlphaString_Length1To1">
        <xs:annotation>
          <xs:documentation xml:lang="en">AF/KL R1 changes  service class like 'S' or 'M' in case of AF/KL.  Usually there for moveable curtain flights where a cabin is divided further into a service class</xs:documentation>
        </xs:annotation>
      </xs:element>
    </xs:sequence>
  </xs:complexType>
  <xs:complexType name="CommercialAgreementsTypeI">
    <xs:annotation>
      <xs:documentation xml:lang="en">To specify commercial agreements between two  or more companies related to joint, shared, lease operations etc.</xs:documentation>
    </xs:annotation>
    <xs:sequence>
      <xs:element name="codeshareDetails" type="CompanyRoleIdentificationTypeI">
        <xs:annotation>
          <xs:documentation xml:lang="en">To convey Marketing/Operating  Flight Details</xs:documentation>
        </xs:annotation>
      </xs:element>
    </xs:sequence>
  </xs:complexType>
  <xs:complexType name="CompanyIdentificationTypeI">
    <xs:annotation>
      <xs:documentation xml:lang="en">Code or name to identify a company and any associated companies.</xs:documentation>
    </xs:annotation>
    <xs:sequence>
      <xs:element name="marketingCompany" type="AlphaNumericString_Length1To3">
        <xs:annotation>
          <xs:documentation xml:lang="en">To convey the Marketing Carrier on the Flight</xs:documentation>
        </xs:annotation>
      </xs:element>
    </xs:sequence>
  </xs:complexType>
  <xs:complexType name="CompanyRoleIdentificationTypeI">
    <xs:annotation>
      <xs:documentation xml:lang="en">To indicate commercial agreements related to the service being provided.</xs:documentation>
    </xs:annotation>
    <xs:sequence>
      <xs:element name="transportStageQualifier" type="AlphaNumericString_Length1To3">
        <xs:annotation>
          <xs:documentation xml:lang="en">To qualify if the flight details given are operating flight details or marketing flight details.</xs:documentation>
        </xs:annotation>
      </xs:element>
      <xs:element name="airlineDesignator" type="AlphaNumericString_Length1To3">
        <xs:annotation>
          <xs:documentation xml:lang="en">airlineDesignator</xs:documentation>
        </xs:annotation>
      </xs:element>
      <xs:element name="flightNumber" type="AlphaNumericString_Length1To4">
        <xs:annotation>
          <xs:documentation xml:lang="en">To convey the marketing/operating  Flight Number</xs:documentation>
        </xs:annotation>
      </xs:element>
      <xs:element name="operationalSuffix" minOccurs="0" type="AlphaNumericString_Length1To1">
        <xs:annotation>
          <xs:documentation xml:lang="en">To convey the marketing/operating FlightSuffix</xs:documentation>
        </xs:annotation>
      </xs:element>
    </xs:sequence>
  </xs:complexType>
  <xs:complexType name="ConfigurationDetailsTypeI">
    <xs:annotation>
      <xs:documentation xml:lang="en">To identify the number of seats for a specific class of service.</xs:documentation>
    </xs:annotation>
    <xs:sequence>
      <xs:element name="classDesignator" type="AlphaString_Length1To1">
        <xs:annotation>
          <xs:documentation xml:lang="en">To indicate cabin class and not an RBD.</xs:documentation>
        </xs:annotation>
      </xs:element>
      <xs:element name="numberOfSeats" type="NumericInteger_Length1To3">
        <xs:annotation>
          <xs:documentation xml:lang="en">Total number of seats in the cabin class. Can be set to 0 if the information is not pertinent.</xs:documentation>
        </xs:annotation>
      </xs:element>
    </xs:sequence>
  </xs:complexType>
  <xs:complexType name="DummySegmentTypeI">
    <xs:annotation>
      <xs:documentation xml:lang="en">To serve the purpose of a mandatory segment at the beginning of a group and to avoid segment collision.</xs:documentation>
    </xs:annotation>
    <xs:sequence />
  </xs:complexType>
  <xs:complexType name="EquipmentIdentificationType">
    <xs:sequence>
      <xs:element name="meanTypeCode" type="AlphaNumericString_Length1To8">
        <xs:annotation>
          <xs:documentation xml:lang="en">Transport identification, coded IATA code of the aircraft type (subtype in the SSIM manual)</xs:documentation>
        </xs:annotation>
      </xs:element>
      <xs:element name="characteristics" type="AlphaNumericString_Length1To3">
        <xs:annotation>
          <xs:documentation xml:lang="en">Configuration Code of the Aircraft.</xs:documentation>
        </xs:annotation>
      </xs:element>
      <xs:element name="identifier" minOccurs="0" type="AlphaNumericString_Length1To17">
        <xs:annotation>
          <xs:documentation xml:lang="en">Aircraft registration number (conditional as this info is not always present in FM)</xs:documentation>
        </xs:annotation>
      </xs:element>
    </xs:sequence>
  </xs:complexType>
  <xs:complexType name="EquipmentInformationTypeI">
    <xs:annotation>
      <xs:documentation xml:lang="en">To specify configuration details of an aircraft.</xs:documentation>
    </xs:annotation>
    <xs:sequence>
      <xs:element name="configurationDetails" minOccurs="0" maxOccurs="5" type="ConfigurationDetailsTypeI">
        <xs:annotation>
          <xs:documentation xml:lang="en">The configuration details : cabin codes and capacities.</xs:documentation>
        </xs:annotation>
      </xs:element>
      <xs:element name="cabinVersion" minOccurs="0" type="AlphaNumericString_Length1To12">
        <xs:annotation>
          <xs:documentation xml:lang="en">To convey the Aircraft Cabin Version Identification</xs:documentation>
        </xs:annotation>
      </xs:element>
      <xs:element name="description" minOccurs="0" type="AlphaNumericString_Length1To70">
